START -> COMMAND_LIST
COMMAND_LIST -> COMMAND COMMAND_LIST2
COMMAND_LIST2 -> COMMAND COMMAND_LIST2 
	       | @
COMMAND -> SYSTEM_COMMAND
	 | DDL_COMMAND 
	 | DML_COMMAND
SYSTEM_COMMAND -> CREATE_DATABASE_COMMAND
		| DROP_DATABASE_COMMAND
		| COMMIT_COMMAND
		| SAVE_COMMAND
		| LOAD_DATABASE_COMMAND
DDL_COMMAND -> CREATE_TABLE_COMMAND
	     | DROP_TABLE_COMMAND
DML_COMMAND -> INSERT_INTO_COMMAND
	     | DELETE_FROM_COMMAND
	     | UPDATE_COMMAND
	     | W_UPDATE_COMMAND
	     | SELECT_COMMAND
	     | W_SELECT_COMMAND
CREATE_DATABASE_COMMAND -> create database DATABASE_NAME ;
DROP_DATABASE_COMMAND -> drop database DATABASE_NAME ;
COMMIT_COMMAN -> SAVE_COMMAND
SAVE_COMMAND -> save ;
LOAD_DATABASE_COMMAND -> load database DATABASE_NAME ;
CREATE_TABLE_COMMAND -> create table TABLE_NAME ( FIELD_DEF_LIST ) ;
DROP_TABLE_COMMAND -> drop table TABLE_NAME ;
INSERT_INTO_COMMAND -> insert into TABLE_NAME INSERT_INTO_COMMAND2
INSERT_INTO_COMMAND2 -> ( FIELD_DEF_LIST ) VALUES ( LITERAL LITERAL_LIST ) ;
		      | VALUES ( LITERAL_LIST ) ;
DELETE_FROM_COMMAND -> delete from TABLE_NAME DELETE_FROM_COMMAND2
DELETE_FROM_COMMAND2 -> where CONDITION ; 
		      | ;
UPDATE_COMMAND -> TABLE_NAME set FIELD_NAME = EXPRESSION UPDATE_COMMAND2
UPDATE_COMMAND2 -> , FIELD_NAME = EXPRESSION UPDATE_COMMAND2
		 | UPDATE_COMMAND3
UPDATE_COMMAND3 -> where CONDITION ;
		 | ;
W_UPDATE_COMMAND -> wUPDATE TABLE_NAME set W_UPDATE_COMMAND2
W_UPDATE_COMMAND2 -> DATE_TIME W_UPDATE_COMMAND3
		   | W_UPDATE_COMMAND3
W_UPDATE_COMMAND3 -> FIELD_NAME = EXPRESSION W_UPDATE_COMMAND4 
W_UPDATE_COMMAND4 -> , W_UPDATE_COMMAND5
W_UPDATE_COMMAND5 -> DATE_TIME 
SELECT_COMMAND ->
W_SELECT_COMMAND ->
DATABASE_NAME ->
TABLE_NAME ->
FIELD_DEF_LIST ->
FIELD ->
FIELD_NAME -> 
LITERAL_LIST ->
LITERAL ->
DATE_TIME -> 


// working with this in a bit
Table_name -> ( field-def  );
Field-def -> field-def name  field-type | field-def’
Field-def’-> ,field-def | EMPTY
Field-def name -> name
Field-type -> [ TYPE ]
TYPE -> null | not null | DATE | integer | number | character
DROP TABLE-> table_name;
INSERT INTO-> table_name | VALUES | 
VALUES-> literal | ,literal
DELETE FROM-> table | WHERE
WHERE-> CONDITION | ,CONDITION
CONDITION-> field-name, operation, condition-name
DATE -> timestamp
